{
    "name": "cronofy/cronofy",
    "description": "PHP wrapper for Cronofy's unified calendar API",
    "keywords": [
        "cronofy",
        "calendar",
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authentication"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Cronofy\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Cronofy\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "beta"
}
