{
    "name": "cronofy/cronofy",
    "description": "PHP wrapper for Cronofy's unified calendar API",
    "keywords": [
        "cronofy",
        "calendar",
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authentication"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^6.2 || ^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "friendsofphp/php-cs-fixer": "^2.10.0"
    },
    "autoload": {
        "psr-4": {
            "Cronofy\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Cronofy\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "beta"
}
